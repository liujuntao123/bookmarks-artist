{
  "name": "bookmarks-artist",
  "displayName": "Bookmarks Artist",
  "version": "0.0.1",
  "description": "A bookmarks manager for Chrome",
  "author": "liujuntao123",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "framer-motion": "^11.11.9",
    "plasmo": "0.89.3",
    "polished": "^4.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-feather": "^2.0.10",
    "styled-components": "^6.1.13"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "chrome_url_overrides": {
      "bookmarks": "./tabs/bookmarks.html"
    },
    "permissions": [
      "bookmarks"
    ]
  }
}
